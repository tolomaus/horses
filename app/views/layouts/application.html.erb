<!DOCTYPE html>
<html <%= yield :html_attributes %>>
<head <%= yield :head_attributes %>>
  <meta charset="utf-8">
  <%= yield :head %>
  <title><%= title %></title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield :body_init %>

<h1>
  <%= image_tag("whitehorse_square.jpg", :alt => FACEBOOK_CONFIG['app_name'], :width => 100, :height => 100) %>
  <%=	FACEBOOK_CONFIG['app_name'] %>
</h1>

<%= yield %>

</body>
</html>
